#!/bin/bash
[ "${debug}" = "true" ] && set -x

function hlp(){
( cat << EOF
    onn starts all containers on the command line
EOF
) >> ${logfile} 2>&1
}

function doc(){
( cat << EOF
$(hlp)
EOF
) >> ${logfile} 2>&1
}

function onn(){
    for CONTAINER in ${CONTAINERS}
    do
        SUFFIX=${CONTAINER##*-}
        [ ! "${SUFFIX}" = "model" ] && lxc-start ${CONTAINER}
    done
}

$CMD
